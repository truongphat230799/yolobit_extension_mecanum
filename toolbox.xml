<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
  <category name="Mecanum Robot" colour="#cb2026" icon="road">
    <block type="i2c_motor_driver">
        <value name="speed">
            <shadow type="math_number">
                <field name="NUM">50</field>
            </shadow>
        </value>
    </block>
    <block type="i2c_motor_delay">
      <value name="speed">
        <shadow type="math_number">
          <field name="NUM">50</field>
        </shadow>
      </value>
      <value name="time">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="i2c_move_motor">
      <value name="motor1">
        <shadow type="math_number">
          <field name="NUM">50</field>
        </shadow>
      </value>
      <value name="motor2">
        <shadow type="math_number">
          <field name="NUM">50</field>
        </shadow>
      </value>
      <value name="motor3">
        <shadow type="math_number">
          <field name="NUM">50</field>
        </shadow>
      </value>
      <value name="motor4">
        <shadow type="math_number">
          <field name="NUM">50</field>
        </shadow>
      </value>
    </block>
    <block type="mecanum_stop" />
    <block type="mecanum_line_sensor_read_all" />
    <block type="mecanum_line_sensor_read_single" />
    <block type="mecanum_ultrasonic_create"/>
    <block type="mecanum_ultrasonic_read"/>
    <block type="mecanum_ultrasonic_checkdistance">
      <value name="DISTANCE">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block> 
  </category>
</xml>